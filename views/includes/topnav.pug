.header.clearfix
  nav.navbar.navbar-expand-md.navbar-light.bg-light.fixed-top
    .container
      a.navbar-brand(href="/") JBrite
      button.navbar-toggler(type="button", data-toggle="collapse", data-target="#navbarsExampleDefault", aria-controls="navbarsExampleDefault", aria-expanded="false", aria-label="Toggle navigation")
        span.navbar-toggler-icon

      #navbarsExampleDefault.collapse.navbar-collapse
        ul.navbar-nav.ml-auto
          li.nav-item
            a.nav-link(href="/events") BROWSE EVENT
          //if(currentUser._id.equals('5a11ae950dab1424b80ae4d5'))
            li.nav-item
                a.nav-link(href='/admin') Admin
           else
          li.nav-item.dropdown
            a.nav-link.dropdown-toggle(data-toggle="dropdown" aria-haspopup="true" aria-expanded="true") Organize
            div.dropdown-menu
              a.dropdown-item(href="/organize/overview") Overview
              a.dropdown-item(href="/organize/pricing") Pricing
              a.dropdown-item(href="/organize/blog") Blog
              a.dropdown-item(href="/organize/resources") Resources
          if (!currentUser)
            li.nav-item
              a.nav-link(href='/signin') Signin 
            li.nav-item
              a.nav-link(href='/users/new') Signup
          else
            li.nav-item
              a.nav-link(href=`/users/${currentUser._id}`) My Profile
            li.nav-item
              a.nav-link(href='/signout') Signout
          li.nav-item
              a.nav-link(href='/events/new') Create Event